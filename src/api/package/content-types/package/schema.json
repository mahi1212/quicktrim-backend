{
  "kind": "collectionType",
  "collectionName": "packages",
  "info": {
    "singularName": "package",
    "pluralName": "packages",
    "displayName": "packages"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "text",
      "required": true
    },
    "price": {
      "type": "text",
      "required": true
    },
    "time": {
      "type": "text",
      "required": true
    },
    "image": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    },
    "shop": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::shop.shop"
    }
  }
}
